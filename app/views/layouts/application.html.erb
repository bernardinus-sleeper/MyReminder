<!DOCTYPE html>
<html>
<head>
  <title>MyReminder</title >

  <%= stylesheet_link_tag 'gmaps4rails' %>

   <%= javascript_include_tag 'jquery-latest.js' %>
  <%= javascript_include_tag 'jquery.tablesorter.js' %>
  <%= javascript_include_tag 'jquery.tablesorter.pager.js' %> 
  
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>

  <%= csrf_meta_tags %>

  <%= javascript_include_tag :defaults %>

  <script type="text/javascript" id="js">
    $(document).ready(function() {
        $("#my_table")
        .tablesorter({widthFixed: true, widgets: ['zebra']})
        .tablesorterPager({container: $("#pager")});
      });
  </script>

<%= stylesheet_link_tag "jquery-ui-1.9.0.custom"%>
<%= javascript_include_tag "jquery-ui-1.9.0.custom"%>





<%= yield :headers %>
</head>
<body>

<%= yield %>
<%= yield :scripts %>
</body>

</html>
